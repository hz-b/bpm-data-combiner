record(longin, "$(PREFIX):count")
{
    field(DESC, "Copy counter from neighbour")
    field(INP, "$(REFERENCE):count CPP")
    field(VAL, "-1")
    field(PINI, "YES")
}

record(ao, "$(PREFIX):freeBytes")
{
    field(DESC, "mimic free bytes")
    # read for MDIZ1T8G:freeBytes
    field(VAL, "11603544")
    field(PREC, 0)
    field(PINI, "YES")
}
