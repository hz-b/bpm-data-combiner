   field(ME00, "7")
   field(IN01, "$(PREFIX):mls:bpm:nams:sec:4")
   field(ME01, "8")
   field(IN02, "$(PREFIX):mls:bpm:nams:sec:1")
   field(ME02, "8")
   field(IN03, "$(PREFIX):mls:bpm:nams:sec:2")
   field(ME03, "8")
   field(IN04, "$(PREFIX):mls:bpm:nams:sec:3:end PP")
   field(ME04, "1")
}
