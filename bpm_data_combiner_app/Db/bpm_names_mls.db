   field(INP, "$(PREFIX):mls:bpm:nams:sec:3")
   field(FTVL, "STRING")
   field(INDX, 1)
   field(NELM, 7)
   field(MALM, 8)

}

record(subArray, "$(PREFIX):mls:bpm:nams:sec:3:end")
{
